{
  "manifest_version": 2,
  "name": "Simple Quick Note",
  "version": "1.0",
  "icons": {
    "128": "images/icons/note-128x128.png",
    "48": "images/icons/note-48x48.png",
    "32": "images/icons/note-32x32.png",
    "16": "images/icons/note-16x16.png"
  },
  "browser_action": {
    "default_icon": "images/icons/note-16x16.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
